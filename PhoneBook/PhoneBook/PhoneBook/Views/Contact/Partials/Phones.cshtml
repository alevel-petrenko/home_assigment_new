@model List<Logic.Models.Phone>

<h3>Phones</h3>

@if (Model != null)
{
    <table>
        <tr>
            <td></td>
            <td>Type</td>
            <td>Number</td>
        </tr>
        @foreach (var phone in Model)
        {
            <tr>
                <td width="75">@Html.ActionLink("Update", "Update", "Phones", new { id = phone.Id }, null)</td>
                <td width="75">@phone.Type</td>
                <td width="150">@phone.Number</td>
            </tr>
        }
    </table>
}
