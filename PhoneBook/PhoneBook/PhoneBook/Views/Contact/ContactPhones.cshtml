@model Logic.Models.Contact

@{
    ViewBag.Title = "Contact details";
}

<h3>Contact details</h3>
<table>
    <tr>
        <td>
            <h4>Name:</h4>
        </td>
        <td>
            @Html.DisplayFor(x => x.Name)
        </td>
    </tr>
    <tr>
        <td>
            <h4>Email:</h4>
        </td>
        <td>
            @Html.DisplayFor(x => x.Email)
        </td>
    </tr>
</table>
<hr />

@{
    Html.RenderPartial("Partials/Phones_with_link", Model.Phones);
}